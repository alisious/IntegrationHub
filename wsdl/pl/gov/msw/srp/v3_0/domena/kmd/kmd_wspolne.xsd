    </xsd:simpleType>
    <xsd:simpleType name="StatusWeryfikacji">
        <xsd:annotation>
            <xsd:documentation>Zbiór dopuszczalnych wartości opisujących status weryfikacji danych.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="POZYTYWNA">
                <xsd:annotation>
                    <xsd:documentation>Pozytywna</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="NEGATYWNA">
                <xsd:annotation>
                    <xsd:documentation>Negatywna</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BRAK">
                <xsd:annotation>
                    <xsd:documentation>Wartość nie weryfikowana</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>
