<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:kmd="http://msw.gov.pl/srp/v3_0/domena/kmd/"
            targetNamespace="http://msw.gov.pl/srp/v3_0/domena/kmd/" version="3.0">
    <xsd:include schemaLocation="kmd_slowniki.xsd"/>
    <xsd:simpleType name="LiniaAdresu">
        <xsd:annotation>
            <xsd:documentation>Zawiera 1 linię adresu korespondenycjnego.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:maxLength value="100"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="AdresKrajowy">
        <xsd:annotation>
            <xsd:documentation>Zawiera dane adresu krajowego zgodnie z art.26 ustawy o ewidencji ludności
                z 24 września 2010r.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="adresZameldowaniaId" type="xsd:integer" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Unikalny identyfikator</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="numerDomu" type="kmd:NumerDomu" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Numer domu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="gmina" type="kmd:Gmina" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Zawiera nazwę gminy z kodem terytorialnym.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="kodPocztowy" type="kmd:KodPocztowy" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Kod pocztowy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="numerLokalu" type="kmd:NumerLokalu" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Numer lokalu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="miejscowoscDzielnica" type="kmd:Miejscowosc" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Zawiera nazwę miejscowości (dzielnicy) z kodem miejscowości i kodem terytorialnym
                        ze słownika TERYT.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ulica" type="kmd:Ulica" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Zawiera nazwę ulicy o strukturze zgodnej ze słownikiem TERYT (plik ULIC) wraz z
                        kodem ulicy z tego słownika.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="wojewodztwo" type="kmd:Wojewodztwo" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Zawiera nazwę województwa z kodem województwa ze słownika TERYT.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="komentarz" type="kmd:Komentarz" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Zawiera komentarz.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AdresKrajowyOut">
        <xsd:annotation>
            <xsd:documentation>Zawiera dane adresu krajowego zgodnie z art.26 ustawy o ewidencji ludności
