                </xsd:annotation>
            </xsd:element>
            <xsd:element name="numerDomu" type="kmd:NumerDomuOut" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Numer domu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="gmina" type="kmd:GminaOut" minOccurs="0">
