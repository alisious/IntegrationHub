            <xsd:element name="gmina" type="dow:Gmina">
                <xsd:annotation>
                    <xsd:documentation>Nazwa gminy z kodem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="powiat" type="dow:Powiat">
                <xsd:annotation>
                    <xsd:documentation>Nazwa powiatu z kodem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="wojewodztwo" type="dow:Wojewodztwo">
                <xsd:annotation>
                    <xsd:documentation>Nazwa wojewodztwa z kodemu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="SeriaDokumentuTozsamosci">
        <xsd:annotation>
            <xsd:documentation>Seria dokumentu tozsamosci.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{2,3}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="SeriaINumerDokumentuTozsamosci">
        <xsd:annotation>
            <xsd:documentation>Typ okreslajacy serie i numer dowodu osobistego. Jest to numer unikalny skladajacy sie z
                serii i numeru.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="seriaDokumentuTozsamosci" type="dow:SeriaDokumentuTozsamosci" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Seria dokumentu tozsamosci.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="numerDokumentuTozsamosci" type="dow:NumerDokumentuTozsamosci" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Numer dokumentu tozsamosci.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
