            </xsd:enumeration>
            <xsd:enumeration value="REKLAMACJA_BLAD_JAKOSCIOWY">
                <xsd:annotation>
                    <xsd:documentation>Reklamacja-błąd jakościowy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DOWOD_NIE_ODEBRANY">
                <xsd:annotation>
                    <xsd:documentation>Dowód nie odebrany</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KRADZIEZ_TOZSAMOSCI">
                <xsd:annotation>
                    <xsd:documentation>Kradzież tożsamości</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="INNE_PRZYCZYNY">
                <xsd:annotation>
                    <xsd:documentation>Inne przyczyny</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UTRATA_DOWODU">
                <xsd:annotation>
                    <xsd:documentation>Utrata dowodu osobistego</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ZMIANA_WIZERUNKU">
                <xsd:annotation>
                    <xsd:documentation>Zmiana wizerunku twarzy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="REKLAMACJA">
                <xsd:annotation>
                    <xsd:documentation>Reklamacja</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ZGON">
                <xsd:annotation>
                    <xsd:documentation>Zgon</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UTRATA_OBYWATELSTWA">
                <xsd:annotation>
                    <xsd:documentation>Utrata obywatelstwa</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PRZEKAZANIE_ZNALEZIONEGO_DOWODU">
