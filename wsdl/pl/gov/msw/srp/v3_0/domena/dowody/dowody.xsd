                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BRAK_CERTYFIKATU_IDENTYFIKACJI_LUB_CERTYFIKATU_PODPISU_OSOBISTEGO">
