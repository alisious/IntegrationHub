                    <xsd:documentation>Przyjęty przez urząd</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ODMOWA_ART_52">
                <xsd:annotation>
                    <xsd:documentation>Odmowa art. 52</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="WYDANY">
                <xsd:annotation>
                    <xsd:documentation>Wydany</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
