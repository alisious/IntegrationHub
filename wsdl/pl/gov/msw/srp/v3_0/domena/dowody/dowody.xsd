            </xsd:enumeration>
            <xsd:enumeration value="BRAK_MOZLIWOSCI_IDENTYFIKACJI_I_UWIERZYTELNIENIA_LUB_ZLOZENIA_PODPISU_OSOBISTEGO">
                <xsd:annotation>
                    <xsd:documentation>
                        Brak możliwości identyfikacji i uwierzytelnienia lub złożenia podpisu osobistego
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="WYMIANA_DOWODU_BEZ_ODCISKOW_PALCOW">
                <xsd:annotation>
                    <xsd:documentation>Wymiana dowodu bez odcisków palców</xsd:documentation>
                </xsd:annotation>
