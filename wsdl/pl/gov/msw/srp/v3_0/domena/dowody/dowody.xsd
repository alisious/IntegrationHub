                <xsd:annotation>
                    <xsd:documentation>Unieważniony - ręczny</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="SPERSONALIZOWANY">
                <xsd:annotation>
                    <xsd:documentation>Spersonalizowany</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ODMOWA_ART_32">
                <xsd:annotation>
                    <xsd:documentation>Odmowa art. 32</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ODWOLANIE">
                <xsd:annotation>
                    <xsd:documentation>Odwołanie</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ZAWIESZONY">
                <xsd:annotation>
                    <xsd:documentation>Zawieszony</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
