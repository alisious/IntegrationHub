        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="POWODY_MEDYCZNE">
                <xsd:annotation>
                    <xsd:documentation>Powody medyczne</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="POWODY_TECHNICZNE">
