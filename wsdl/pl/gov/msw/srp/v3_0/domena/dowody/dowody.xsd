                    <xsd:documentation>Dowód nie odebrany</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KRADZIEZ_TOZSAMOSCI">
                <xsd:annotation>
                    <xsd:documentation>Kradzież tożsamości</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="INNE_PRZYCZYNY">
                <xsd:annotation>
                    <xsd:documentation>Inne przyczyny</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UTRATA_DOWODU">
                <xsd:annotation>
                    <xsd:documentation>Utrata dowodu osobistego</xsd:documentation>
