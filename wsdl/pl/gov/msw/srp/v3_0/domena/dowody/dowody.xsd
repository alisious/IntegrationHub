                    <xsd:documentation>Reklamacja</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PO_RAZ_PIERWSZY">
                <xsd:annotation>
                    <xsd:documentation>Po raz pierwszy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UPLYW_TERMINU_ZAWIESZENIA_DOWODU_OSOBISTEGO">
                <xsd:annotation>
                    <xsd:documentation>Upływ terminu zawieszenia dowodu osobistego</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="WYMIANA_DOWODU_BEZ_WARSTWY_ELEKTRONICZNEJ">
                <xsd:annotation>
                    <xsd:documentation>Wymiana dowodu bez warstwy elektronicznej</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BRAK_UWIERZYTELNIENIA_LUB_PODPISU">
                <xsd:annotation>
                    <xsd:documentation>
