                    <xsd:documentation>Utrata obywatelstwa</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ART_32">
                <xsd:annotation>
                    <xsd:documentation>Utrata obywatelstwa</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ART_52">
                <xsd:annotation>
                    <xsd:documentation>Utrata obywatelstwa</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="USZKODZENIE">
                <xsd:annotation>
                    <xsd:documentation>Uszkodzenie</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UPLYW_TERMINU_ZAWIESZENIA">
