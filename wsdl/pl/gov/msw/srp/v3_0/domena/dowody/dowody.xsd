                    <xsd:documentation>Wojewoda</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BURMISTRZ_MIASTA">
                <xsd:annotation>
                    <xsd:documentation>Burmistrz miasta</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BURMISTRZ_MIASTA_I_GMINY">
                <xsd:annotation>
                    <xsd:documentation>Burmistrz miasta i gminy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BURMISTRZ_GMINY">
                <xsd:annotation>
                    <xsd:documentation>Burmistrz gminy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BURMISTRZ_GMINY_DZIELNICY">
                <xsd:annotation>
                    <xsd:documentation>Burmistrz gminy-dzielnicy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BURMISTRZ_DZIELNICY">
                <xsd:annotation>
                    <xsd:documentation>Burmistrz dzielnicy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PREZYDENT_MIASTA">
                <xsd:annotation>
                    <xsd:documentation>Prezydent miasta</xsd:documentation>
                </xsd:annotation>
