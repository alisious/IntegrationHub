        <xsd:annotation>
            <xsd:documentation>Typ definiujacy wysylke dokumentu do organu gminy.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="statusWysylki" type="dow:StatusWysylki" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Status wysylki.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataWysylki" type="kmd:DataKalendarzowa" nillable="true">
