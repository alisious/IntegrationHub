                <xsd:annotation>
                    <xsd:documentation>KWMO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KPMO">
                <xsd:annotation>
                    <xsd:documentation>KPMO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KMMO">
                <xsd:annotation>
                    <xsd:documentation>KMMO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KSMO">
                <xsd:annotation>
                    <xsd:documentation>KSMO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KDMO">
                <xsd:annotation>
                    <xsd:documentation>KDMO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KSMO">
                <xsd:annotation>
                    <xsd:documentation>KSMO</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="NACZELNIK_PREZYDENT_MIASTA_POWIATU">
