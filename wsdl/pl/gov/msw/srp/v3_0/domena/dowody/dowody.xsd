                <xsd:annotation>
                    <xsd:documentation>Nowy</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PRZYJETY_PRZEZ_SPD">
                <xsd:annotation>
                    <xsd:documentation>Przyjęty przez SPD</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="NIEDOPUSZCZONY_DO_PRODUKCJI">
                <xsd:annotation>
                    <xsd:documentation>Niedopuszczony do produkcji</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="SPERSONALIZOWANY">
                <xsd:annotation>
                    <xsd:documentation>Spersonalizowany</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ODWOLANIE">
                <xsd:annotation>
                    <xsd:documentation>Odwołanie</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ZAKONCZONY_ODMOWNIE">
