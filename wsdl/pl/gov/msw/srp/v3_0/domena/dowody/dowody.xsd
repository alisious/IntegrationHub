            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BLAD_TECHNICZNY">
                <xsd:annotation>
                    <xsd:documentation>Brak weryfikacji z powodu błędu technicznego</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="WYDAWANIE_POZA_URZEDEM">
                <xsd:annotation>
                    <xsd:documentation>Brak weryfikacji z powodu wydawania dowodu poza urzędem</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PROBLEMY_MEDYCZNE">
                <xsd:annotation>
                    <xsd:documentation>Brak weryfikacji z powodu problemów medycznych</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
