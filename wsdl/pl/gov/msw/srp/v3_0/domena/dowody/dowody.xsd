                <xsd:annotation>
                    <xsd:documentation>Odmowa art. 32</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BEZ_ROZPOZNANIA">
                <xsd:annotation>
                    <xsd:documentation>Bez rozpoznania</xsd:documentation>
