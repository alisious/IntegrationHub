        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Obywatelstwo">
        <xsd:annotation>
            <xsd:documentation>Typ określający obywatelstwo osoby ubiegającej się o wydanie dowodu osobistego.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="POLSKIE">
                <xsd:annotation>
                    <xsd:documentation>Polskie obywatelstwo.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="INNE">
                <xsd:annotation>
                    <xsd:documentation>Obywatelstwo innego kraju.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ZnakiLacinskie">
        <xsd:annotation>
            <xsd:documentation>Zawiera duze i male znaki lacinskie wraz z wszystkimi dopuszczonymi modyfikatorami
                (akcentami), kropke, myslnik, spacje, przecinek, ukosnik.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="PriorytetZlecenia">
        <xsd:annotation>
            <xsd:documentation>Typ definujący priorytet zlecenia.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="dow:ZnakiLacinskie">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="TrescZlecenia">
        <xsd:annotation>
            <xsd:documentation>Zawartosc zlecenia.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:maxLength value="3000"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="DaneOdmowy">
        <xsd:annotation>
            <xsd:documentation>
                Dane odmowy zgodny z artykułem 32.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="numerDecyzji" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        Numer decyzji odmowy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataDecyzji"
                         type="kmd:DataKalendarzowa">
                <xsd:annotation>
                    <xsd:documentation>
                        Data decyzji odmowy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="urzadDecyzji" type="dow:KodTerytorialny">
                <xsd:annotation>
                    <xsd:documentation>
                        Kod terytorialny organu(urzędu) dokonującego odmowy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="urzadDecyzjiNazwa" type="dow:NazwaOrganu" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Nazwa organu(urzędu) dokonującego odmowy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneNiewaznosci">
        <xsd:annotation>
            <xsd:documentation>
                Dane unieważnienia dowodu zgodna z artykułem 52.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="numerDecyzji" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        Numer decyzji unieważnienia.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataDecyzji" type="kmd:DataKalendarzowa">
                <xsd:annotation>
                    <xsd:documentation>
                        Data decyzji unieważnienia.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="urzadDecyzji" type="dow:KodTerytorialny">
                <xsd:annotation>
                    <xsd:documentation>
                        Kod terytorialny organu(urzędu) dokonującego unieważnienia.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="nazwaOrganu" type="dow:NazwaOrganu">
                <xsd:annotation>
                    <xsd:documentation>
                        Nazwa organu unieważniającego dowód.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="IdUrzad">
        <xsd:annotation>
            <xsd:documentation>Typ opisujacy techniczny identyfikator urzędu w bazie danych. Wykorzystywany w
                uslugach wyszukiwania, aktualizacji i udostepniania danych dowodów osobistych i wniosków.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IdOrgan">
        <xsd:annotation>
            <xsd:documentation>Typ opisujacy techniczny identyfikator organu w bazie danych. Wykorzystywany w
                uslugach wyszukiwania, aktualizacji i udostepniania danych dowodów osobistych i wniosków.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="NazwiskoJednoczlonowe">
        <xsd:annotation>
            <xsd:documentation>Typ definiujacy nazwisko jednoczłonowe.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="dow:ZnakiLacinskie">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="300"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="PrzyczynaBezRozpoznaniaWniosek">
        <xsd:annotation>
            <xsd:documentation>Typ opisujacy przyczynę bez rozpoznania dla wniosku.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="kmd:ZnakiSRP">
            <xsd:maxLength value="300"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="CzyIstniejeWniosekAktywny">
        <xsd:annotation>
            <xsd:documentation>Typ określa czy wybrana osoba posiada zarejestrowany i aktywny wniosek.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:boolean"/>
    </xsd:simpleType>
    <xsd:simpleType name="NazwaUrzeduWniosekAktywny">
        <xsd:annotation>
            <xsd:documentation>Typ przechowuje nazwę urzędu w którym zarejestrowano wniosek.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="10"/>
            <xsd:maxLength value="200"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="NazwiskoUzytkownika">
        <xsd:annotation>
            <xsd:documentation>Typ definiujacy czlon nazwiska uzytkownika
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="dow:ZnakiLacinskie">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ImieUzytkownika">
        <xsd:annotation>
            <xsd:documentation>Typ definiujacy czlon imie uzytkownika
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="dow:ZnakiLacinskie">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="DaneStatusuWniosku">
        <xsd:annotation>
            <xsd:documentation>Typ opisujacy dane statusu wniosku o wydanie dowodu osobistego.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="statusWniosku" type="dow:StatusWniosku">
                <xsd:annotation>
                    <xsd:documentation>Status przetwarzania danych wniosku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataNadaniaStatusu" type="kmd:DataICzas">
                <xsd:annotation>
                    <xsd:documentation>Data nadania statusu wniosku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="imieUzytkownika" type="dow:ImieUzytkownika">
                <xsd:annotation>
                    <xsd:documentation>Użytkownik, który nadał status wniosku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="nazwiskoUzytkownika" type="dow:NazwiskoUzytkownika">
                <xsd:annotation>
                    <xsd:documentation>Użytkownik, który nadał status wniosku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="CzyIstniejeDowodAktywny">
        <xsd:annotation>
            <xsd:documentation>Typ określa czy wybrana osoba posiada aktywny dowód osobisty.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:boolean"/>
    </xsd:simpleType>
    <xsd:simpleType name="NazwaUrzeduDowodAktywny">
        <xsd:annotation>
            <xsd:documentation>Typ przechowuje nazwę urzędu, w którym wydano dowód osobisty.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="10"/>
            <xsd:maxLength value="200"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="DaneStatusuDowodu">
        <xsd:annotation>
            <xsd:documentation>Typ opisujacy dane statusu dowodu osobistego.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="statusDowodu" type="dow:StatusDowodu">
                <xsd:annotation>
                    <xsd:documentation>Status przetwarzania danych dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataNadaniaStatusu" type="kmd:DataICzas">
                <xsd:annotation>
                    <xsd:documentation>Data nadania statusu dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataWykonaniaAkcji" type="kmd:DataICzas">
                <xsd:annotation>
                    <xsd:documentation>Data wykonania akcji zmiany statusu dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="imieUzytkownika" type="dow:ImieUzytkownika">
                <xsd:annotation>
                    <xsd:documentation>Użytkownik, który nadał status dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="nazwiskoUzytkownika" type="dow:NazwiskoUzytkownika">
                <xsd:annotation>
                    <xsd:documentation>Użytkownik, który nadał status dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="nazwaOrganu" type="dow:NazwaOrganu">
                <xsd:annotation>
                    <xsd:documentation>Nazwa organu użytkownika, który nadał status dowodu osobistego.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Wydruk">
        <xsd:annotation>
            <xsd:documentation>Typ opisujący wydruk.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dane" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Dane dla wydruku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="IdDowodu">
        <xsd:annotation>
            <xsd:documentation>Typ określający techniczny identyfikator danych dowodu w bazie RDO.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer"/>
    </xsd:simpleType>
    <xsd:simpleType name="IdWniosku">
        <xsd:annotation>
            <xsd:documentation>Typ określający techniczny identyfikator danych wniosku w bazie RDO.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer"/>
    </xsd:simpleType>
    <xsd:complexType name="ZmienStatusDowoduNaWyslanyParametry">
        <xsd:annotation>
            <xsd:documentation>Typ okreslajacy parametry zmiany statusu dowodu na wysłany.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element type="dow:IdWniosku" name="idWniosku"/>
            <xsd:element type="xsd:date" name="dataWyslaniaDoGminy"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ListaDaneDowodu">
        <xsd:annotation>
            <xsd:documentation>
                Lista dowodów osobistych
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="daneDowodu" type="dow:DaneDowodu" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="dataZmiany" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Data zmiany, od której pobrane zostały zmiany.
                        Jeśli parametr nie został zwrócony to są to dane inicjujące.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ListaDowodowSubskrypcja">
        <xsd:annotation>
            <xsd:documentation>
                Lista danych dowodów osobistych do subskrypcji.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="DaneDowodow" type="dow:DaneDowodow" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="dataNastepnegoWywolania" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Data do wywołania kolejnej subskrypcji.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ListaDaneWniosku">
        <xsd:annotation>
            <xsd:documentation>
                Lista wniosków
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="daneWniosku" type="dow:DaneWniosku" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="dataZmiany" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Data zmiany, od której pobrane zostały zmiany.
                        Jeśli parametr nie został zwrócony to są to dane inicjujące.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="WersjaElementu">
        <xsd:annotation>
            <xsd:documentation>Przechowuje informacje o wersji elementu. Informacja o wersji elementu jest
                wykorzystywana przez usługi aktualizujące dane w rejestrze RDO.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer"/>
    </xsd:simpleType>
    <xsd:simpleType name="StatusWarstwyEdo">
        <xsd:annotation>
            <xsd:documentation>Status warstwy elektronicznej dowodu osobistego.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ZAWIESZONA">
                <xsd:annotation>
                    <xsd:documentation>Zawieszona</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UNIEWAZNIONA">
                <xsd:annotation>
                    <xsd:documentation>Unieważniona</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="WAZNA">
                <xsd:annotation>
                    <xsd:documentation>Ważna</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="BRAK">
                <xsd:annotation>
                    <xsd:documentation>Brak</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="DaneDowoduDoUniewaznienia">
        <xsd:annotation>
            <xsd:documentation>
                Dane dowodu do unieważnienia.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="seriaNumerDowodu" type="dow:SeriaINumerDokumentuTozsamosci" minOccurs="0"/>
            <xsd:element type="xsd:date" name="dataWaznosci" minOccurs="0"/>
            <xsd:element type="xsd:date" name="dataUniewaznieniaCertyfikatow"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneZawieszenia">
        <xsd:annotation>
            <xsd:documentation>
                Dane zawieszenia dowodu.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataZawieszenia" type="kmd:Czas"/>
            <xsd:element name="organZawieszajacyDowod" type="dow:NazwaOrganu"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneCofnieciaZawieszenia">
        <xsd:annotation>
            <xsd:documentation>
                Dane zawieszenia dowodu.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataCofnieciaZawieszenia" type="kmd:Czas"/>
            <xsd:element name="organCofajacyZawieszenieDowodu" type="dow:NazwaOrganu"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneZgloszeniaZwrotu">
        <xsd:annotation>
            <xsd:documentation>
                Dane odnalezienia dowodu.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataZgloszeniaZwrotu" type="kmd:Data"/>
            <xsd:element name="organRejestrujacyZwrot" type="dow:NazwaOrganu"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneStatusuDowoduIWarstwyElektronicznej">
        <xsd:sequence>
            <xsd:element name="statusDowodu" type="dow:StatusDowodu">
                <xsd:annotation>
                    <xsd:documentation>
                        Status dowodu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="statusWarstwyEdo" type="dow:StatusWarstwyEdo">
                <xsd:annotation>
                    <xsd:documentation>
                        Status warstwy elektronicznej.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataICzasCofnieciaZawieszenia" type="kmd:Czas" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Data i czas zawieszenia dowodu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataICzasZawieszenia" type="kmd:Czas" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Data i czas zawieszenia dowodu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneWarstwyElektronicznej">
        <xsd:annotation>
            <xsd:documentation>
                Dane warstwy elektronicznej.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="zdjecieKolorowe" type="xsd:base64Binary" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Zdjęcie kolorowe.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="statusWarstwyEdo" type="dow:StatusWarstwyEdo" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Status warstwy elektronicznej.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataWaznosciZObwieszczenia" type="kmd:DataKalendarzowa" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Data ważności z obwieszczenia.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="daneZawieszenia" type="dow:DaneZawieszenia" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Dane zawieszenia.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="daneCofnieciaZawieszenia" type="dow:DaneCofnieciaZawieszenia" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Dane cofnięcia zawieszenia.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneDowoduSubskrypcjaIn">
        <xsd:annotation>
            <xsd:documentation>Typ określający zestaw pól wejściowych dla usługi dodawania dowodów do subskrypcji.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="seriaINumer" type="dow:SeriaINumerDokumentuTozsamosci">
                <xsd:annotation>
                    <xsd:documentation>Seria i numer dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="pesel" type="kmd:Pesel">
                <xsd:annotation>
                    <xsd:documentation>Numer PESEL powiązany z dokumentem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DaneDowoduSubskrypcjaOut">
        <xsd:annotation>
            <xsd:documentation>Typ określający zestaw pól wyjściowych dla usługi dodawania dowodów do subskrypcji.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="seriaINumer" type="dow:SeriaINumerDokumentuTozsamosci">
                <xsd:annotation>
                    <xsd:documentation>Seria i numer dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="czyDodano" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>Informacja, czy dokument został dodany do subskrypcji.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
