                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UTRATA_DOKUMENTU_TOZSAMOSCI_KRADZIEZ">
                <xsd:annotation>
                    <xsd:documentation>Utrata dokumentu tożsamości-kradzież</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ZNISZCZENIE">
                <xsd:annotation>
                    <xsd:documentation>Zniszczenie</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ZMIANA_DANYCH">
                <xsd:annotation>
                    <xsd:documentation>Zmiana danych zawartych w dowodzie osobistym</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="REKLAMACJA_BLEDNE_DANE">
