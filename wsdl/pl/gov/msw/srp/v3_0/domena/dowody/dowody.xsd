                </xsd:annotation>
            </xsd:element>
            <xsd:element name="pesel" type="dow:Pesel" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Numer Pesel urzednika</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
