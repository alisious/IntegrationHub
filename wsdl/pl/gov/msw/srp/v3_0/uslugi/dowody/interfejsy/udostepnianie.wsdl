<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="udostepnianie" targetNamespace="http://msw.gov.pl/srp/v3_0/uslugi/dowody/"
                  xmlns:tns="http://msw.gov.pl/srp/v3_0/uslugi/dowody/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <wsdl:types>
        <xsd:schema targetNamespace="http://msw.gov.pl/srp/v3_0/uslugi/dowody/"
                    xmlns:dow="http://msw.gov.pl/srp/v3_0/domena/dowody/"
                    xmlns:ubase="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/udostepnianieBase/"
                    xmlns:wsp="http://msw.gov.pl/srp/v3_0/uslugi/wspolne/"
                    xmlns:xsd_2="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/udostepnianie/"
                    xmlns:wys="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/wyszukiwanie/"
                    xmlns:pes="http://msw.gov.pl/srp/v3_0/domena/pesel/"
                    xmlns:kmd="http://msw.gov.pl/srp/v3_0/domena/kmd/">
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/domena/kmd/"
                        schemaLocation="../../../domena/kmd/kmd.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/udostepnianie/"
                        schemaLocation="../parametry/udostepnianie.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/uslugi/wspolne/"
                        schemaLocation="../../../uslugi/wspolne/wspolne.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/"
                        schemaLocation="../parametry/parametry.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/domena/dowody/"
                        schemaLocation="../../../domena/dowody/dowody.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/domena/pesel/"
                        schemaLocation="../../../domena/pesel/pesel.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/wyszukiwanie/"
                        schemaLocation="../parametry/wyszukiwanie.xsd"/>
            <xsd:import namespace="http://msw.gov.pl/srp/v3_0/uslugi/dowody/parametry/udostepnianieBase/"
                        schemaLocation="../../../uslugi/dowody/parametry/udostepnianieBase.xsd"/>
            <xsd:element name="bladBiznesowy" type="wsp:BladBiznesowy"/>
            <xsd:element name="bladTechniczny" type="wsp:BladTechniczny"/>

            <xsd:element name="udostepnijHistorieStatusuWniosku">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="idWniosku" type="dow:IdWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Id wniosku</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijHistorieStatusuWnioskuResponse">
                <xsd:complexType>
                    <xsd:annotation>
                        <xsd:documentation>Dane reprezentujące dane poprzednich statusów wniosku.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:sequence>
                        <xsd:element name="numerWniosku" type="dow:NumerWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Numer wniosku na podstawie którego został wydany dowód.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="idWniosku" type="dow:IdWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Id wniosku</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="listaPoprzednichStatusowWniosku" type="dow:DaneStatusuWniosku" minOccurs="0"
                                     maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Pole okreslajace zestaw poprzednich statusów wniosku, wypelnione
                                    jesli pole statusu wniosku kiedykolwiek bylo zmienione i posiada poprzednika.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneDowoduPoNumerze">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="idDowodu" type="dow:IdDowodu">
                            <xsd:annotation>
                                <xsd:documentation>Numer dowodu do pobrania.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneDowoduPoNumerzeResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="dowod" type="dow:DaneDowodu" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Pełne dane dowodu tożsamości.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="idWniosku" type="dow:IdWniosku" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Id wniosku</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="numerWniosku" type="dow:NumerWniosku" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Numer wniosku na podstawie którego został wydany dowód.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneDowoduPoId">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="idDowodu" type="dow:IdDowodu">
                            <xsd:annotation>
                                <xsd:documentation>Numer dowodu do pobrania.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="udostepnijZdjecie" type="xsd:boolean">
                            <xsd:annotation>
                                <xsd:documentation>Wartość true/false określająca czy należy przekazywać w zwracanych
                                    danych zawartość pola zdjęcie.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="udostepnijPodpis" type="xsd:boolean">
                            <xsd:annotation>
                                <xsd:documentation>Wartość true/false określająca czy należy przekazywać w zwracanych
                                    danych zawartość pola podpis.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="udostepnijDaneTekstowe" type="xsd:boolean">
                            <xsd:annotation>
                                <xsd:documentation>Wartość true/false określająca czy należy przekazywać w zwracanych
                                    danych zawartości pól tekstowych.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneDowoduPoIdResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="dowod" type="dow:DaneDowodu" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Pełne dane dowodu tożsamości.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="dostepDoWykazuUniewaznionychDowodowPoSeriaINumer">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="seriaINumer" type="dow:SeriaINumerDokumentuTozsamosci">
                            <xsd:annotation>
                                <xsd:documentation>Seria i numer dowodu osobistego.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="dostepDoWykazuUniewaznionychDowodowPoSeriaINumerResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="dowodZnajdujeSieNaLiscie" type="xsd:boolean">
                            <xsd:annotation>
                                <xsd:documentation>Rezultat czy dowód znajduje się na liście unieważnionych dowodów.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneDowodowPoNumerze">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="listaNumerowDowodow" type="xsd_2:ListaNumerowDowodow">
                            <xsd:annotation>
                                <xsd:documentation>Lista numerów dowodów do zwrócenia.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneDowodowPoNumerzeResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="dowody" type="xsd_2:ListaDowodowZUdostepnianymiDanymi">
                            <xsd:annotation>
                                <xsd:documentation>Lista pełnych danych z dowodów tożsamości.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWniosku">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="ubase:udostepnijDaneWnioskuBase"/>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskuWS">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="ubase:udostepnijDaneWnioskuBase"/>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskuResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="wniosek" type="dow:DaneWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Dane pobranego wniosku.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskuUkr">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="idOsoby" type="pes:IdDaneOsoby">
                            <xsd:annotation>
                                <xsd:documentation>Id osoby w rejestrze PESEL.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskuUkrResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="wniosek" type="dow:DaneWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Dane pobranego wniosku.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="daneFormularzaWnioskuUkr" type="dow:DaneFormularzaWnioskuUkr">
                            <xsd:annotation>
                                <xsd:documentation>Dane specyficzne dla wniosku uchodźcy UKR.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskow">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="ubase:udostepnijDaneWnioskowBase"/>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskowWS">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="ubase:udostepnijDaneWnioskowBase"/>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneWnioskowResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="wnioski" type="xsd_2:ListaWnioskow">
                            <xsd:annotation>
                                <xsd:documentation>Lista pobranych wniosków.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneAktualnegoDowoduPoPesel">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="numerPesel" type="dow:Pesel">
                            <xsd:annotation>
                                <xsd:documentation>Numer pesel osoby, której dowód jest zwracany.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="requestId" type="wsp:IdentyfikatorZadania" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Unikalny identyfikator żądania.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneAktualnegoDowoduPoPeselResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="dowod" type="dow:DaneDowodu" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Aktualne dane dowodu tożsamości.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="idWniosku" type="dow:IdWniosku" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Id wniosku</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="numerWniosku" type="dow:NumerWniosku" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Numer wniosku na podstawie którego został wydany dowód.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneAktualnychDowodowPoPesel">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="listaNumerowPesel" type="xsd_2:ListaNumerowPesel">
                            <xsd:annotation>
                                <xsd:documentation>Lista numerów pesel osób, których dowody mają zostać zwrócone.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDaneAktualnychDowodowPoPeselResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="dowody" type="xsd_2:ListaDowodowZUdostepnianymiDanymi">
                            <xsd:annotation>
                                <xsd:documentation>Lista aktualnych danych z dowodów tożsamości.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijStatusWniosku">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="numerWniosku" type="dow:NumerWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Numer wniosku</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijStatusWnioskuResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="statusWniosku" type="dow:StatusWniosku">
                            <xsd:annotation>
                                <xsd:documentation>Status wniosku.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="statusDowodu" type="dow:StatusDowodu">
                            <xsd:annotation>
                                <xsd:documentation>Status dowodu.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="dataPrzyjeciaKopertyPuk" type="xsd:date" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Data przyjęcia koperty z PUK.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="dataWydania" type="dow:Data" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Data odbioru dokumentu przez obywatela.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijWniosekAktywnyPoPesel">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="pesel" type="dow:Pesel">
                            <xsd:annotation>
                                <xsd:documentation>Numer ewidencyjny osoby w rejestrze PESEL
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijWniosekAktywnyPoPeselResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="czyIstniejeWniosekAktywny" type="dow:CzyIstniejeWniosekAktywny">
                            <xsd:annotation>
                                <xsd:documentation>Wartość typu boolean wskazująca czy wybrana osoba posiada aktywny
                                    wniosek.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="daneWniosku" type="dow:DaneWniosku" nillable="true">
                            <xsd:annotation>
                                <xsd:documentation>Dane istniejącego i aktywnego wniosku.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="nazwaUrzeduWniosekAktywny" type="dow:NazwaUrzeduWniosekAktywny"
                                     nillable="true">
                            <xsd:annotation>
                                <xsd:documentation>Nazwa urzędu w którym istnieje aktywny wniosek.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDowodAktywnyPoPesel">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="pesel" type="dow:Pesel">
                            <xsd:annotation>
                                <xsd:documentation>Numer ewidencyjny osoby w rejestrze PESEL.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijDowodAktywnyPoPeselResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="czyIstniejeDowodAktywny" type="dow:CzyIstniejeDowodAktywny">
                            <xsd:annotation>
                                <xsd:documentation>Wartość typu boolean wskazująca czy wybrana osoba posiada aktywny
                                    dowód osobisty.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="daneDowodu" type="dow:DaneDowodu" nillable="true">
                            <xsd:annotation>
                                <xsd:documentation>Dane istniejącego i aktywnego dowodu osobistego.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="nazwaUrzeduDowodAktywny" type="dow:NazwaUrzeduDowodAktywny" nillable="true">
                            <xsd:annotation>
                                <xsd:documentation>Nazwa urzędu, w którym istnieje aktywny dowód osobisty.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijHistorieStatusuDowodu">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="idDowodu" type="dow:IdDowodu">
                            <xsd:annotation>
                                <xsd:documentation>Numer dowodu osobistego.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijHistorieStatusuDowoduResponse">
                <xsd:complexType>
                    <xsd:annotation>
                        <xsd:documentation>Dane reprezentujące dane poprzednich statusów wniosku.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:sequence>
                        <xsd:element name="seriaINumer" type="dow:SeriaINumerDokumentuTozsamosci">
                            <xsd:annotation>
                                <xsd:documentation>Numer dowodu osobistego.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="listaPoprzednichStatusowDowodu" type="dow:DaneStatusuDowodu" minOccurs="0"
                                     maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Pole okreslajace zestaw poprzednich statusów dowodu, wypelnione jesli
                                    pole statusu dowodu kiedykolwiek bylo zmienione i posiada poprzednika.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijOstatniDowodPoOsobaId">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="idOsoby" type="pes:IdDaneOsoby">
                            <xsd:annotation>
                                <xsd:documentation>Id osoby w rejestrze PESEL.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijOstatniDowodPoOsobaIdResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="daneDowodu" type="dow:DaneDowodu" nillable="true">
                            <xsd:annotation>
                                <xsd:documentation>Dane ostatniego dowodu osobistego,
                                    o ile taki istnieje.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijWnioskiWydanieDowoduOsobistego">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="kryteriaUdostepnianiaWnioski" type="xsd_2:kryteriaUdostepnianiaWnioski">
                            <xsd:annotation>
                                <xsd:documentation>Kryteria udostępniania wniosków.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="czyWyszukiwanieZGwiazdka" type="xsd:boolean" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Wartość true/false określająca czy wyszukiwanie stringów z gwiazdką.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="czyTeletransmisja" type="xsd:boolean" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Wartość true/false określająca czy wyszukiwanie dotyczy teletransmisji.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="udostepnijWnioskiWydanieDowoduOsobistegoResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="listaWnioskow" type="wys:ListaZnalezionychWnioskow">
                            <xsd:annotation>
                                <xsd:documentation>Lista znalezionych wniosków.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
