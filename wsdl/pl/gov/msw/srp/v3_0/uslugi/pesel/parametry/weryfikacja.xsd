<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://msw.gov.pl/srp/v3_0/uslugi/pesel/parametry/weryfikacja/" version="3.0" xmlns:kmd="http://msw.gov.pl/srp/v3_0/domena/kmd/" xmlns:pes="http://msw.gov.pl/srp/v3_0/domena/pesel/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsd_1="http://msw.gov.pl/srp/v3_0/uslugi/pesel/parametry/weryfikacja/">
    <xsd:import namespace="http://msw.gov.pl/srp/v3_0/domena/kmd/" schemaLocation="../../../domena/kmd/kmd.xsd"/>
    <xsd:import namespace="http://msw.gov.pl/srp/v3_0/domena/pesel/" schemaLocation="../../../domena/pesel/pesel.xsd"/>
    <xsd:complexType name="WeryfikowaneDaneDowodu">
        <xsd:annotation>
            <xsd:documentation>Typ służący do przekazywania danych dowodu osobistego w celach weryfikacji.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataWaznosci" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Data ważności dokumentu tożsamości.
                        Opis wyświetlany na interfejsie użytkownika dla danego pola: Data ważności.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="seriaINumer" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Seria i numer dokumentu.
                        Opis wyświetlany na interfejsie użytkownika dla danego pola: Seria i numer.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="wystawca" type="kmd:Organ" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Pole zawierające oznaczenie wystawcy dowodu osobistego.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="WeryfikowaneDaneDowoduWynik">
        <xsd:annotation>
            <xsd:documentation>Typ służący do przekazywania danych dowodu osobistego w celach weryfikacji.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataWaznosci" type="kmd:StatusWeryfikacji">
                <xsd:annotation>
                    <xsd:documentation>Data ważności dokumentu tożsamości.
                        Opis wyświetlany na interfejsie użytkownika dla danego pola: Data ważności.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="seriaINumer" type="kmd:StatusWeryfikacji">
                <xsd:annotation>
                    <xsd:documentation>Seria i numer dokumentu.
                        Opis wyświetlany na interfejsie użytkownika dla danego pola: Seria i numer.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="wystawca" type="kmd:StatusWeryfikacji">
                <xsd:annotation>
                    <xsd:documentation>Pole zawierające oznaczenie organu wydającego dowód osobisty.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="WeryfikowaneDanePaszportu">
        <xsd:annotation>
            <xsd:documentation>Typ służący do przekazywania danych paszportu w celach weryfikacji.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataWaznosci" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Data ważności dokumentu tożsamości.
                        Opis wyświetlany na interfejsie użytkownika dla danego pola: Data ważności.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="seriaINumer" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Seria i numer dokumentu.
                        Opis wyświetlany na interfejsie użytkownika dla danego pola: Seria i numer.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="WeryfikowaneDanePaszportuWynik">
        <xsd:annotation>
            <xsd:documentation>Typ służący do przekazywania danych paszportu w celach weryfikacji.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataWaznosci" type="kmd:StatusWeryfikacji">
                <xsd:annotation>
                    <xsd:documentation>Data ważności dokumentu tożsamości.
