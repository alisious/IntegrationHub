    <xsd:complexType name="WeryfikowaneDanePobytu">
        <xsd:annotation>
            <xsd:documentation>Typ służący do przekazywania danych pobytu w
                celach weryfikacji.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="numerDomu" type="xsd:string"
                         minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Numer domu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="kodPocztowy" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Kod pocztowy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="numerLokalu" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Numer lokalu.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="miejscowosc" type="xsd:string"
                         minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Zawiera nazwę miejscowości (dzielnicy) z kodem
                        miejscowości i kodem terytorialnym ze słownika TERYT.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ulica" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Zawiera nazwę ulicy o strukturze zgodnej ze
                        słownikiem TERYT (plik ULIC) wraz z kodem ulicy z tego słownika.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataDo" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Pole określające datę wymeldowania z adresu,
                        przy którym jest wprowadzone.
                        Jeśli data jest wypełniona to dany adres nie jest aktualny.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataOd" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Pole określające datę zameldowania pod danym
                        adresem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataUplywuDeklarowanegoTerminu"
                         type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Pole określające datę upływu zadeklarowanego
                        pobytu pod danym adresem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
