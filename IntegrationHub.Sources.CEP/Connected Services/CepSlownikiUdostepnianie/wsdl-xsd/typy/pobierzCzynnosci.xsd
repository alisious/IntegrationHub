	<xsd:include schemaLocation="common.xsd"/>
	<xsd:complexType name="PobierzCzynnosci">
		<xsd:sequence>
			<xsd:element name="identyfikatory" type="slo:Identyfikatory" minOccurs="0"/>
			<xsd:element name="modul" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
