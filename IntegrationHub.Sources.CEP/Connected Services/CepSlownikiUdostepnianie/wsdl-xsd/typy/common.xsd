		<xsd:complexContent>
			<xsd:extension base="slo:WartoscZakresowaD">
				<xsd:sequence>
					<xsd:element name="jednostka" type="slo:WartoscSlownikowa" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
