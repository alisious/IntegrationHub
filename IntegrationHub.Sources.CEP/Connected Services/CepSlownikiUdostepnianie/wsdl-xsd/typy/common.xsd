	<xsd:complexType name="WartoscZakresowaD">
		<xsd:annotation>
			<xsd:documentation>
                Typ opisujący wartość ze zdefiniowanym zakresem wartości. Dla pola odpowiadającego takiej wartości dozwolone są jedyne wartości X, takie, że
                wartoscDolnaZakresu &#8804; X oraz wartoscGornaZakresu &#8805; X.
                Pole wartoscTypowa określa proponowaną wartość pola.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="wartoscTypowa" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="wartoscDolnaZakresu" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="wartoscGornaZakresu" type="xsd:decimal" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Stronicowanie">
		<xsd:sequence>
			<xsd:element name="pozycja">
				<xsd:annotation>
					<xsd:documentation>Numeracja stron rozpoczyna się od "1"</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="liczbaStron">
				<xsd:annotation>
					<xsd:documentation>parametr określający liczbę wszystkich dostępnych stron, numeracja rozpoczyna się od "1"</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:long">
						<xsd:minInclusive value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JednostkaTerytorialna">
		<xsd:sequence>
